@page "/"
@inject IJSRuntime JsRuntime;

<button class="btn btn-primary" @onclick="@callJs">Call JS</button>

@code {
    async void callJs()
    {
        var r = await JsRuntime.InvokeAsync<string>(
                "myfunc",
                "Hello from Blazor");
        Console.WriteLine(r);
    }
}
